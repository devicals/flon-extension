{
  "name": "flexion-language",
  "displayName": "Flexion Language Support",
  "description": "Syntax highlighting, IntelliSense, and dynamic expressions for Flexion (.flon) files",
  "version": "1.9.2",
  "publisher": "error-dev",
  "engines": {
    "vscode": "^1.75.0"
  },
  "main": "./extension.js",
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "keywords": [
    "flexion",
    "flon",
    "data",
    "serialization"
  ],
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/devicals"
  },
  "contributes": {
    "languages": [
      {
        "id": "flexion",
        "aliases": [
          "Flexion",
          "flon"
        ],
        "extensions": [
          ".flon"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "icon.png",
          "dark": "icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "flexion",
        "scopeName": "source.flon",
        "path": "./syntaxes/flon.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "flexion",
        "path": "./snippets/flon.json"
      }
    ],
    "commands": [
      {
        "command": "flexion.openWiki",
        "title": "Flexion: Open Wiki"
      },
      {
        "command": "flexion.jumpToWikiSection",
        "title": "Flexion: Jump to Wiki Section"
      },
      {
        "command": "flexion.openExample",
        "title": "Flexion: Open Example"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "flexion-explorer",
          "title": "Flexion",
          "icon": "sidebar.svg"
        }
      ]
    },
    "views": {
      "flexion-explorer": [
        {
          "id": "flexion-help",
          "name": "Help and Feedback",
          "icon": "sidebar.svg"
        }
      ]
    },
    "menus": {
      "commandPalette": [
        {
          "command": "flexion.openWiki",
          "when": "false"
        },
        {
          "command": "flexion.jumpToWikiSection",
          "when": "false"
        },
        {
          "command": "flexion.openExample",
          "when": "false"
        }
      ]
    }
  }
}