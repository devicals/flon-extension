!! Javascript Object Notation | JSON (.json)

{
	"id": "12345-abc-def",
	"isActive": true,
	"age": 35,
	"name": "Jane Doe",
	"address": {
		"street": "123 Main St",
		"city": "Anytown",
		Code": "12345",
		"isRural": false
	},
	"phoneNumbers": [
		{
			"type": "home",
			"number": "555-1234"
		},
		{
			"type": "mobile",
			"number": "555-5678"
		}
	],
	"tags": ["json", "example", "data", "structure"],
